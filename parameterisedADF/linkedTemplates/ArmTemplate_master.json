{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "parameterisedADF"
		},
		"LS_ADLS_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://dadfadls.azuredatalakestore.net/webhdfs/v1"
		},
		"LS_ADLS_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "f77e87f9-ae31-4f5b-a45e-fbdac0102e34"
		},
		"LS_ADLS_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "3d450ca0-cad9-4d0a-aaf8-6cdbf740f6db"
		},
		"LS_ADLS_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "dadftest"
		},
		"LS_AKV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dadfkv.vault.azure.net/"
		},
		"LS_ASQL_Generic_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@{linkedService().SecretName}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_dataLakeStoreUri')]"
					},
					"LS_ADLS_properties_typeProperties_tenant": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_tenant')]"
					},
					"LS_ADLS_properties_typeProperties_subscriptionId": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_subscriptionId')]"
					},
					"LS_ADLS_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_resourceGroupName')]"
					},
					"LS_AKV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AKV_properties_typeProperties_baseUrl')]"
					},
					"LS_ASQL_Generic_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_Generic_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}